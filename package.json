{
	"name": "pu2",
	"version": "0.0.36",
	"description": "A library written in TypeScript",
	"keywords": [],
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/gregolai/pu2"
	},
	"author": "Gregory Dalton <gregolai@gmail.com>",
	"license": "MIT",
	"engines": {
		"node": ">=12.10",
		"npm": ">=6.11"
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist/"
	],
	"main": "dist/esm/index.js",
	"module": "dist/esm/index.js",
	"config": {},
	"scripts": {
		"build": "node ./scripts build",
		"clean": "node ./scripts clean",
		"format:fix": "npm run format -- --write",
		"format": "prettier \"src/**/*.{ts,tsx,js,jsx,css}\"",
		"postrelease": "npm run release:github && npm run release:npm",
		"prerelease": "npm run build",
		"release:github": "git push --no-verify --follow-tags origin master",
		"release:npm": "npm publish .",
		"release:preflight": "npm pack . --dry-run",
		"release": "standard-version",
		"start": "cd playground && npm start",
		"style:fix": "npm run format:fix",
		"style": "npm run format -- --list-different",
		"test:ci": "npm run test -- --ci",
		"test:coverage": "npm run test -- --coverage",
		"test:watch": "npm run test -- --watch",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run style:fix && npm run test -- --bail --onlyChanged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx,css}": [
			"prettier --write",
			"git add"
		]
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	},
	"peerDependencies": {
		"react": ">=16.13",
		"react-dom": ">=16.13"
	},
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@types/jest": "^26.0.7",
		"@types/lodash": "^4.14.158",
		"@types/node": "^14.0.26",
		"@types/prettier": "^2.0.2",
		"@types/react": "^16.9.43",
		"brotli-size": "^4.0.0",
		"gzip-size": "^5.1.1",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"jest-watch-typeahead": "^0.6.0",
		"kleur": "^3.0.3",
		"lint-staged": "^10.2.11",
		"lodash": "^4.17.19",
		"prettier": "^2.0.5",
		"pretty-bytes": "^5.3.0",
		"standard-version": "^8.0.2",
		"ts-jest": "^26.1.4",
		"typescript": "^3.9.7"
	}
}
