{
	"name": "pu2",
	"version": "0.0.14",
	"description": "A library written in TypeScript",
	"keywords": [],
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/gregolai/pu2"
	},
	"author": "Gregory Dalton <gregolai@gmail.com>",
	"license": "MIT",
	"engines": {
		"node": ">=12.10",
		"npm": ">=6.11"
	},
	"config": {},
	"scripts": {
		"help": "node ./scripts",
		"build": "node ./scripts build",
		"clean": "node ./scripts clean",
		"commit": "git-cz",
		"format:fix": "npm run format -- --write",
		"format": "prettier \"**/*.{ts,tsx,js,jsx,css,scss,sass,less}\"",
		"lint:fix": "npm run lint -- --fix",
		"lint": "tslint --project tsconfig.json --format codeFrame",
		"__OLD:postrelease": "node scripts/copy && node scripts/build && npm run release:github && npm run release:npm",
		"postrelease": "npm run release:github && npm run release:npm",
		"prerelease": "npm run build",
		"release:github": "git push --no-verify --follow-tags origin master",
		"release:npm": "npm publish dist",
		"release:preflight": "npm pack ./dist --dry-run",
		"release": "standard-version",
		"start": "cd playground && npm start",
		"style:fix": "npm run format:fix && npm run lint:fix",
		"style": "npm run format -- --list-different && npm run lint",
		"test:ci": "npm run test -- --ci",
		"test:coverage": "npm run test -- --coverage",
		"test:watch": "npm run test -- --watch",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run style:fix && npm run test -- --bail --onlyChanged"
		}
	},
	"lint-staged": {
		"**/*.{ts,tsx,js,jsx,css,scss,sass,less}": [
			"prettier --write",
			"git add"
		],
		"src/**/*.{ts,tsx}": [
			"git add"
		]
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	},
	"peerDependencies": {
		"tslib": ">=1.10.0"
	},
	"dependencies": {
		"react": "^16.13.0",
		"react-dom": "^16.13.0"
	},
	"devDependencies": {
		"@types/jest": "^25.1.3",
		"@types/node": "^12.12.27",
		"@types/prettier": "^1.19.0",
		"@types/react": "^16.9.22",
		"brotli-size": "^4.0.0",
		"gzip-size": "^5.1.1",
		"husky": "^4.2.3",
		"jest": "^25.1.0",
		"jest-watch-typeahead": "^0.4.2",
		"json5": "^2.1.1",
		"kleur": "^3.0.3",
		"lint-staged": "^10.0.7",
		"lodash": "^4.17.15",
		"prettier": "^1.19.1",
		"pretty-bytes": "^5.3.0",
		"standard-version": "^7.1.0",
		"ts-jest": "^25.2.0",
		"tslib": "^1.10.0",
		"tslint": "^6.0.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^9.0.0",
		"tslint-etc": "^1.10.0",
		"tslint-react": "^4.2.0",
		"typescript": "^3.7.5",
		"typescript-tslint-plugin": "^0.5.5"
	}
}
